package Test;

public class test
{
	public static void main(String[] args) 
	{
		boolean b1 = false;
		for(int i=0;i<5;i++){
			for(int j=0;j<5;j++){
				if(b1 == false)
					System.out.println("*");
				    b1 = true;
			}

		   System.out.println("_");
		   b1 = false;
				}
			}
	}

		
